        "tekstis": {
          "kieli_fi": "",
          "kieli_sv": "<p><span lang=\"SV\">Huvudämnesstudier 65 sp varav kandidatarbetet 10 sp, grundstudier 65 sp, biämnesstudier 20-25 sp, fritt valbara studier 25-30 sp.</span></p>"
        }
      },
